*int build_dictionary(char str){
  int  occur[127]=[];
  int i=0,tmp_char;
  for(i=0;str[i];i++){
    tmp_char = str[i];
    occur[tmp_char]++;
  }
  return occur;
}
  

  int main(void){
    char test[100]="Quisiera volver amarte, volver a quererte volver a tenerte cerca de mi, mis ojos lloran por ti";
    int *oc;
    build_dictionary(test);
}
